{
    "name": "api-starter",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "update": "yarn upgrade-interactive --latest",
        "lint": "eslint \"{src,test-k6}/**/*.ts\"",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:k6": "test-k6/run-tests.sh",
        "prepare": "husky",
        "precommit": "yarn build && yarn test && yarn lint",
        "format": "yarn prettier -w ./src ./test-k6",
        "format:check": "yarn prettier --check ./src ./test-k6",
        "docker-build": "docker build --build-arg NODE_VERSION=$(cat .nvmrc | sed 's/v//') -t api-starter .",
        "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli",
        "migration:create": "yarn typeorm migration:create",
        "migration:generate": "yarn typeorm -d src/database/typeorm-migrations.datasource.ts migration:generate",
        "migration:run": "yarn typeorm -d src/database/typeorm-migrations.datasource.ts migration:run",
        "migration:revert": "yarn typeorm -d src/database/typeorm-migrations.datasource.ts migration:revert"
    },
    "dependencies": {
        "@nestjs/common": "11.1.9",
        "@nestjs/config": "4.0.2",
        "@nestjs/core": "11.1.9",
        "@nestjs/jwt": "11.0.1",
        "@nestjs/passport": "11.0.5",
        "@nestjs/platform-express": "11.1.9",
        "@nestjs/swagger": "11.2.3",
        "@nestjs/terminus": "11.0.0",
        "@nestjs/typeorm": "11.0.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.3",
        "cookie-parser": "1.4.7",
        "date-fns": "4.1.0",
        "helmet": "8.1.0",
        "morgan": "1.10.1",
        "mysql2": "3.15.3",
        "passport": "0.7.0",
        "passport-jwt": "4.0.1",
        "passport-local": "1.0.0",
        "ramda": "0.32.0",
        "reflect-metadata": "0.2.2",
        "rxjs": "7.8.2",
        "typeorm": "0.3.27"
    },
    "devDependencies": {
        "@commitlint/cli": "20.1.0",
        "@commitlint/config-conventional": "20.0.0",
        "@nestjs/cli": "11.0.10",
        "@nestjs/schematics": "11.0.9",
        "@nestjs/testing": "11.1.8",
        "@stylistic/eslint-plugin-ts": "4.4.1",
        "@types/cookie-parser": "1.4.10",
        "@types/express": "5.0.5",
        "@types/jest": "30.0.0",
        "@types/k6": "0.57.1",
        "@types/morgan": "1.9.10",
        "@types/node": "22.15.29",
        "@types/passport-jwt": "4.0.1",
        "@types/passport-local": "1.0.38",
        "@types/ramda": "0.31.1",
        "@types/supertest": "6.0.3",
        "@typescript-eslint/eslint-plugin": "8.46.2",
        "@typescript-eslint/parser": "8.46.2",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "eslint": "9.39.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-functional": "9.0.2",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-nested-if": "1.0.0",
        "eslint-plugin-no-else": "0.2.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "5.5.4",
        "husky": "9.1.7",
        "jest": "30.2.0",
        "prettier": "3.6.2",
        "rimraf": "6.1.0",
        "supertest": "7.1.4",
        "ts-jest": "29.4.5",
        "ts-loader": "9.5.4",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2",
        "wait-on": "9.0.1"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
